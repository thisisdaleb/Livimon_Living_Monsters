                                <Grid.RowDefinitions>
                                    <RowDefinition Height="72*"/>
                                    <RowDefinition Height="23*"/>
                                </Grid.RowDefinitions>
                                <TextBlock x:Name="InvItem1Name" HorizontalAlignment="Left" Margin="5,5,0,0" TextWrapping="Wrap" Text="Potion" VerticalAlignment="Top" FontFamily="Cabin" FontSize="16" Width="135" Foreground="#FFE2E2E2"/>
                                <TextBlock x:Name="InvItem1Amount" HorizontalAlignment="Left" Margin="145,5,0,0" TextWrapping="Wrap" Text="x99" VerticalAlignment="Top" FontFamily="Cabin" FontSize="16" Width="30" Foreground="#FFE2E2E2" RenderTransformOrigin="0.492,1.252"/>
                                <TextBlock x:Name="InvItem2Name" HorizontalAlignment="Left" Margin="5,25,0,0" TextWrapping="Wrap" Text="Ether" VerticalAlignment="Top" FontFamily="Cabin" FontSize="16" Width="135" Foreground="#FFE2E2E2"/>
                                <TextBlock x:Name="InvItem2Amount" HorizontalAlignment="Left" Margin="145,25,0,0" TextWrapping="Wrap" Text="x1" VerticalAlignment="Top" FontFamily="Cabin" FontSize="16" Width="30" Foreground="#FFE2E2E2"/>
                                <TextBlock x:Name="InvItem3Name" HorizontalAlignment="Left" Margin="5,45,0,0" TextWrapping="Wrap" Text="Revive" VerticalAlignment="Top" FontFamily="Cabin" FontSize="16" Width="135" Foreground="#FFE2E2E2"/>
                                <TextBlock x:Name="InvItem3Amount" HorizontalAlignment="Left" Margin="145,45,0,0" TextWrapping="Wrap" Text="x1" VerticalAlignment="Top" FontFamily="Cabin" FontSize="16" Width="30" Foreground="#FFE2E2E2"/>
                                <TextBlock x:Name="InvItem4Name" HorizontalAlignment="Left" Margin="5,65,0,0" TextWrapping="Wrap" Text="Super Revive" VerticalAlignment="Top" FontFamily="Cabin" FontSize="16" Width="135" Foreground="#FFE2E2E2"/>
                                <TextBlock x:Name="InvItem4Amount" HorizontalAlignment="Left" Margin="145,65,0,0" TextWrapping="Wrap" Text="x1" VerticalAlignment="Top" FontFamily="Cabin" FontSize="16" Width="30" Foreground="#FFE2E2E2"/>
                                <TextBlock x:Name="InvItem5Name" HorizontalAlignment="Left" Margin="5,85,0,0" TextWrapping="Wrap" Text="Super Potion" VerticalAlignment="Top" FontFamily="Cabin" FontSize="16" Width="135" Foreground="#FFE2E2E2"/>
                                <TextBlock x:Name="InvItem5Amount" HorizontalAlignment="Left" Margin="145,85,0,0" TextWrapping="Wrap" Text="x1" VerticalAlignment="Top" FontFamily="Cabin" FontSize="16" Width="30" Foreground="#FFE2E2E2"/>
