                                <TextBlock x:Name="InvItem6Amount" HorizontalAlignment="Left" Margin="145,105,0,0" TextWrapping="Wrap" Text="x1" VerticalAlignment="Top" FontFamily="Cabin" FontSize="16" Width="30" Foreground="#FFE2E2E2"/>
