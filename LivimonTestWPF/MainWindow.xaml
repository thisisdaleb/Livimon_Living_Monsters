                                <TextBlock x:Name="InvItem8Amount" HorizontalAlignment="Left" Margin="145,1,0,0" TextWrapping="Wrap" Text="x0" VerticalAlignment="Top" FontFamily="Cabin" FontSize="16" Width="30" Foreground="#FFE2E2E2" Grid.Row="1"/>
